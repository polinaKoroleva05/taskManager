{
    "compilerOptions": {
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "esModuleInterop": true,
        "jsx": "react-jsx",
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@widgets/*": [
                "src/widgets/*"
            ],
            "@shared/*": [
                "src/shared/*"
            ],
            "@store/*": [
                "src/app/store/*"
            ]
        }
    }
}